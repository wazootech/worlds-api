{
  "name": "@wazoo/worlds-server",
  "version": "0.0.1",
  "exports": {
    ".": "./mod.ts",
    "./serve": "./main.ts",
    "./testing": "./lib/testing/mod.ts",
    "./database": "./lib/database/mod.ts",
    "./embeddings": "./lib/embeddings/mod.ts",
    "./blob": "./lib/blob/mod.ts",
    "./errors": "./lib/errors/mod.ts",
    "./rdf-patch": "./lib/rdf-patch/mod.ts"
  },
  "imports": {
    "#/": "./",
    "@fartlabs/rt": "jsr:@fartlabs/rt@^0.0.11",
    "@langchain/textsplitters": "npm:@langchain/textsplitters@^1.0.1",
    "@fartlabs/search-store": "jsr:@fartlabs/search-store@^0.0.2",
    "@comunica/query-sparql-rdfjs-lite": "npm:@comunica/query-sparql-rdfjs-lite@^5.1.3",
    "n3": "npm:n3@^2.0.1",
    "@types/n3": "npm:@types/n3@^1.26.1",
    "@std/http": "jsr:@std/http@^1.0.25",
    "ai": "npm:ai@^4.1.45",
    "@ai-sdk/google": "npm:@ai-sdk/google@^1.1.18",
    "ollama-ai-provider": "npm:ollama-ai-provider@^1.2.0"
  },
  "tasks": {
    "start": "deno serve -A --env ./main.ts",
    "generate": "deno -A jsr:@fartlabs/sql-embedder@0.0.5 ./"
  },
  "publish": {
    "exclude": ["**/*.test.ts", "**/*_test.ts"]
  }
}

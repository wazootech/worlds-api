{
  "imports": {
    "#/": "./src/",
    "@hono/zod-openapi": "npm:@hono/zod-openapi@^1.1.5",
    "@rdfjs/types": "npm:@rdfjs/types@^2.0.1",
    "@scalar/hono-api-reference": "npm:@scalar/hono-api-reference@^0.9.26",
    "@std/fs": "jsr:@std/fs@^1.0.20",
    "@std/http": "jsr:@std/http@^1.0.22",
    "@std/path": "jsr:@std/path@^1.1.3",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/streams": "jsr:@std/streams@^1.0.14",
    "hono": "npm:hono@^4.10.7",
    "n3": "npm:n3@^1.26.0",
    "oxigraph": "npm:oxigraph@^0.5.2"
  },
  "tasks": {
    "start": "deno serve -A --unstable-kv src/main.ts",
    "test": "deno test -A --unstable-kv",
    "bench": "deno bench -A",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check": "deno check",
    "precommit": {
      "description": "Runs fmt, lint, check, and test",
      "dependencies": [
        "test",
        "fmt",
        "lint",
        "check"
      ]
    }
  }
}

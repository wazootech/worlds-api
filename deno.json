{
  "name": "@fartlabs/worlds",
  "version": "0.0.37",
  "exports": {
    ".": "./src/sdk/mod.ts",
    "./internal": "./src/sdk/internal/mod.ts",
    "./tools": "./src/tools/mod.ts"
  },
  "imports": {
    "#/": "./src/",
    "@ai-sdk/google": "npm:@ai-sdk/google@^3.0.13",
    "@ai-sdk/anthropic": "npm:@ai-sdk/anthropic@^3.0.23",
    "@comunica/query-sparql-rdfjs-lite": "npm:@comunica/query-sparql-rdfjs-lite@^5.1.0",
    "@fartlabs/rt": "jsr:@fartlabs/rt@^0.0.11",
    "@fartlabs/search-store": "jsr:@fartlabs/search-store@^0.0.2",
    "@google/genai": "npm:@google/genai@^1.38.0",
    "@kitsonk/kv-toolbox": "jsr:@kitsonk/kv-toolbox@^0.30.0",
    "@libsql/client": "npm:@libsql/client@^0.17.0",
    "@olli/kvdex": "jsr:@olli/kvdex@^3.4.2",
    "@std/assert": "jsr:@std/assert@^1.0.17",
    "@std/cli": "jsr:@std/cli@^1.0.26",
    "@std/ulid": "jsr:@std/ulid@^1.0.0",
    "@tensorflow-models/universal-sentence-encoder": "npm:@tensorflow-models/universal-sentence-encoder@^1.3.3",
    "@tensorflow/tfjs": "npm:@tensorflow/tfjs@^4.22.0",
    "@tensorflow/tfjs-backend-wasm": "npm:@tensorflow/tfjs-backend-wasm@^4.22.0",
    "@types/n3": "npm:@types/n3@^1.26.1",
    "ai": "npm:ai@^6.0.49",
    "cfonts": "npm:cfonts@^3.3.1",
    "n3": "npm:n3@^2.0.1",
    "zod": "npm:zod@^4.3.6",
    "deno_tree_sitter": "https://deno.land/x/deno_tree_sitter@1.0.1.2/main/main.js",
    "common_tree_sitter_languages/": "https://esm.sh/gh/jeff-hykin/common_tree_sitter_languages@8eeb010ee5edb3dea6ac41f98432815e9e70f211/main/"
  },
  "tasks": {
    "start": "deno serve -A --unstable-kv --env --unstable-raw-imports src/server/main.ts",
    "test": "deno test -A --unstable-kv --unstable-raw-imports",
    "publish:dry": "deno publish --dry-run --allow-dirty",
    "fmt": "deno fmt --unstable-sql",
    "lint": "deno lint --unstable-kv --unstable-raw-imports",
    "check": "deno check --unstable-kv --unstable-raw-imports",
    "precommit": {
      "description": "Runs fmt, lint, and test",
      "dependencies": [
        "test",
        "check",
        "lint",
        "fmt"
      ]
    },
    "example": "deno -A --env --unstable-kv --unstable-raw-imports src/tools/example.ts",
    "generate": "deno -A jsr:@fartlabs/sql-embedder@0.0.5 src"
  }
}
